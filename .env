# текущая версия проекта в Docker
TAG=v1.1

# заменяемые переменные окружения, переопределяются значениями переданными в консольке
# указываем какой пакет и какой версии будет (или уже) установлен
PACKAGE=yupe/yupe
BRANCH=1.1

# константы для проекта
APP_NAME=yupe
APP_DIR=app
WEBROOT_DIR=public
DOCKER_SOURCE_DIR=source
# порты по которым будет доступно приложение и адмайнер в различных окружениях
# в dev окружении
EXTERNAL_WEB_DEV_PORT=7771
EXTERNAL_ADMINER_DEV_PORT=7772
# в prod окружении
EXTERNAL_WEB_PROD_PORT=7781
EXTERNAL_ADMINER_PROD_PORT=7782
# в test окружении
EXTERNAL_WEB_TEST_PORT=7791
EXTERNAL_ADMINER_TEST_PORT=7792
